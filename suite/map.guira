let square
  function x [* x x]
let id
  function x x

if
  != [map id [list 1 2 3 4]]
     [list 1 2 3 4]
  abort "fail 1"
  nil
if
  != [map square [list 1 2 3 4]]
     [list 1 4 9 16]
  abort "fail 2"
  nil
if
  != [map square nil]
     nil
  abort "fail 3"
  nil

let squarer
  map square
if
  != squarer:[list 1 2 3 4]
     [list 1 4 9 16]
  abort "fail 4"
  nil

print "map.guira: OK"
