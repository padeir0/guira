if
  not= round:9/10 1
  abort "fail 1"
  nil
if
  not= round:1/10 0
  abort "fail 2"
  nil

if
  not= ceiling:5/10 1
  abort "fail 3"
  nil
if
  not= ceiling:9/10 1
  abort "fail 4"
  nil
if
  not= ceiling:1/10 1
  abort "fail 5"
  nil

if
  not= floor:5/10 0
  abort "fail 6"
  nil
if
  not= floor:9/10 0
  abort "fail 7"
  nil
if
  not= floor:1/10 0
  abort "fail 8"
  nil

if
  not= floor:0 ceiling:0 round:0
  abort "fail 9"
  nil

print "rounding.guira: OK"
