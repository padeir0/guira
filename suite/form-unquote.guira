let a 'b
let ;a 1

if
  not= b 1
  abort "fail 1"
  nil

let multi-quote
  form [a . b]
    pair a b

let c [multi-quote ;a ;b]

if
  not= c '[b 1]
  abort "fail 2"
  nil

# form-unquote is shallow
if
  not=
    multi-quote 1 ;[+ 1 1] ;[+ 1 1 1] '[1 2 ;3]
    '[1 2 3 '[1 2 [form-unquote 3]]]
  abort "fail 3"
  nil

print "form-unquote.guira: OK"
